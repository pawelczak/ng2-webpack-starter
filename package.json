{
  "name": "ng2-webpack-starter",
  "version": "0.0.1",
  "description": "Angular2 webpack starter",
  "keywords": [
    "angular2",
    "webpack",
    "karma",
    "jasmine"
  ],
  "author": "Łukasz Pawełczak",
  "license": "MIT",
  "scripts": {
    "build": "npm run build:dev",
    "build:dev": "webpack --config config/webpack.dev.js --inline --colors --progress --display-error-details --display-cached",
    "build:prod": "webpack --config config/webpack.prod.js --inline --colors --progress --display-error-details --display-cached",
    "watch": "npm run build -- --watch",
    "server": "npm run server:dev",
    "server:dev": "webpack-dev-server --config config/webpack.dev.js --inline --colors --progress --display-error-details --display-cached --port 3000  --content-base src",
    "server:prod": "webpack-dev-server --config config/webpack.prod.js --inline --colors --progress --display-error-details --display-cached --port 3000  --content-base src",
    "start": "npm run server:dev",
    "start:prod": "npm run server:prod",
    "tests": "karma start",
    "postinstall": "typings -- install"
  },
  "dependencies": {
    "@angular/common": "2.0.0-rc.5",
    "@angular/compiler": "2.0.0-rc.5",
    "@angular/core": "2.0.0-rc.5",
    "@angular/http": "2.0.0-rc.5",
    "@angular/platform-browser": "2.0.0-rc.5",
    "@angular/platform-browser-dynamic": "2.0.0-rc.5",
    "@angular/router": "3.0.0-rc.1",
    "core-js": "^2.2.0",
    "es6-shim": "^0.35.1",
    "highcharts": "^4.2.6",
    "ie-shim": "^0.1.0",
    "jquery": "^1.7.4",
    "reflect-metadata": "^0.1.8",
    "rxjs": "5.0.0-beta.6",
    "zone.js": "~0.6.12"
  },
  "devDependencies": {
    "angular2-template-loader": "^0.4.0",
    "awesome-typescript-loader": "^1.1.1",
    "css-loader": "^0.23.1",
    "html-webpack-plugin": "^2.22.0",
    "jasmine-core": "^2.4.1",
    "jquery": "^1.7.4",
    "karma": "^1.2.0",
    "karma-jasmine": "^1.0.2",
    "karma-phantomjs-launcher": "^1.0.1",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^1.8.0",
    "null-loader": "^0.1.1",
    "phantomjs-prebuilt": "^2.1.12",
    "raw-loader": "^0.5.1",
    "to-string-loader": "^1.1.4",
    "ts-helpers": "^1.1.1",
    "tslint": "^3.14.0",
    "tslint-loader": "^2.1.5",
    "typescript": "~1.8.9",
    "typings": "~1.0.3",
    "webpack": "^1.12.9",
    "webpack-dev-server": "^1.14.0",
    "webpack-merge": "^0.8.4"
  }
}
